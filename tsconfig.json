{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "exactOptionalPropertyTypes": false,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "module": "ES2022",
    "target": "ES2022",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
